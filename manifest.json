{
  "name": "Feedly CSS Override",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Override the default Feedly styles",
  "homepage_url": "https://www.google.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://feedly.com/*"],
      "css": ["css/inject.css"]
    }
  ]
}
